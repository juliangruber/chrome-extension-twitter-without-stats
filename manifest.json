{
  "manifest_version": 2,
  "name": "Simple HTML Styler",
  "short_name": "Simple HTML",
  "version": "0.0.0",
  "description": "style webpages that use simple HTML",
  "author": "K.E. Mitchell",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "onload.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "options_page": "options.html"
}
